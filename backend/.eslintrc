{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"plugins": [
		"@typescript-eslint"
	],
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/eslint-recommended",
		"plugin:@typescript-eslint/recommended",
		"./.eslintrc.shared"
	],
	"rules": {
		"indent": ["error", "tab"],
		"comma-dangle": ["error", "always-multiline"],
	}
}
